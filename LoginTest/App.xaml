<?xml version = "1.0" encoding = "UTF-8" ?>
    <Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:material="clr-namespace:UraniumUI.Material.Resources;assembly=UraniumUI.Material"
             xmlns:m="clr-namespace:UraniumUI.Material.Resources;assembly=UraniumUI.Material"
             xmlns:local="clr-namespace:LoginTest"
             x:Class="LoginTest.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="appColors" Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary x:Name="appStyles" Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
    <Application.MainPage>
        <NavigationPage>
            <x:Arguments>
                <Shell>
                    <ShellContent Route="RoutesPage"
                                  Title="Routes"
                                  Icon="home.png"
                                  ContentTemplate="{DataTemplate local:RoutesPage}" />
                </Shell>
            </x:Arguments>
        </NavigationPage>
    </Application.MainPage>
</Application>